{
  "name": "ctf-archive",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "compile": "cross-env NODE_ENV=production vite build",
    "font": "node font.js",
    "check": "run-p check:*",
    "check:svelte": "svelte-check --threshold error --human",
    "check:ts": "tsc --noEmit",
    "watch": "run-p watch:*",
    "watch:svelte": "svelte-check --threshold error --human --watch",
    "watch:ts": "tsc --noEmit --watch",
    "start": "run-p watch:ts font dev",
    "build": "run-s check:ts font compile",
    "preview": "vite preview",
    "serve": "run-s build && find dist -type f -not -name '*.woff2' -not -name '*.jpg' -not -name '*.png' -not -name '*.webp' | xargs brotli && ssh ubuntu@ctf-archive.com rm -rf /ctf/www/* && rsync -rpoghvz --chown=ubuntu:web --chmod=F0640,D750 dist/ ubuntu@ctf-archive.com:/ctf/www/"
  },
  "devDependencies": {
    "@roxi/routify": "^3.0.0-next.80",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "broadcast-channel": "^4.13.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.1.0",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "subset-font": "^1.4.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "xxh32": "^1.4.0"
  },
  "dependencies": {
    "@sveltestack/svelte-query": "^1.6.0",
    "@toast-ui/editor": "^3.1.7",
    "dequal": "^2.0.2",
    "fflate": "^0.7.3",
    "svelte-i18n": "^3.4.0",
    "svelte-meta-tags": "^2.6.1"
  }
}
