{
  "name": "ctf-archive",
  "version": "0.0.1",
  "@comments scripts": {
    "dev": "develop with blazing fast rebuilds",
    "build": "run build scripts below",
    "build:app": "build single page application (SPA)",
    "nollup": "run the nollup no-bundler",
    "routify": "run routify"
  },
  "scripts": {
    "dev": "run-p routify nollup",
    "build": "run-s build:*",
    "build:app": "routify -b && rollup -c",
    "nollup": "nollup -c",
    "routify": "routify",
    "serve": "run-s build:* && find dist -type f -not -name '*.woff2' -not -name '*.jpg' -not -name '*.png' -not -name '*.webp' | xargs brotli && ssh ubuntu@ctf-archive.com rm -rf /ctf/www/* && rsync -rpoghvz --chown=ubuntu:web --chmod=F0640,D750 dist/ ubuntu@ctf-archive.com:/ctf/www/",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@roxi/routify": "^2.18.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/dompurify": "^2.2.3",
    "@types/marked": "^3.0.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "nollup": "^0.18.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup": "^2.56.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^1.0.0-8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.8.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.0-dev.20210902"
  },
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "dependencies": {
    "@sveltestack/svelte-query": "^1.4.1",
    "dequal": "^2.0.2",
    "dompurify": "^2.3.1",
    "fflate": "^0.7.1",
    "marked": "^3.0.2",
    "svelte-i18n": "^3.3.10",
    "svelte-meta-tags": "^2.1.0"
  }
}
